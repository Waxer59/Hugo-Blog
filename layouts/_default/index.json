{{- $result := slice -}}

{{- range (where .Site.RegularPages "Section" "!=" "") -}}
    {{- $data := dict "url" .Permalink "title" .Title "date" (dict "day" .Date.Day "month" (i18n .Date.Month) "year" .Date.Year) "readTime" .Params.readTime "technology" .Params.technology "content"  (.Content | plainify) -}}
    {{- $result = $result | append $data -}}
{{- end -}}

{{ jsonify $result }}